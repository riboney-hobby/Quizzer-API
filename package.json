{
  "name": "quizzer-fullstack",
  "version": "1.0.0",
  "description": "Web application for quizzes",
  "main": "./src/index.js",
  "scripts": {
    "dev": "NODE_ENV=DEV node ./src/index.js",
    "test": "NODE_ENV=TEST jest --verbose --runInBand",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepare": "npx husky-init && husky install && npx husky add .husky/pre-commit \"npx lint-staged\""
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix"
    ],
    ".{json, md, html, js, jsx}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/riboney/Quizzer-Fullstack.git"
  },
  "author": "ironbe#4809",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/riboney/Quizzer-Fullstack/issues"
  },
  "homepage": "https://github.com/riboney/Quizzer-Fullstack#readme",
  "dependencies": {
    "axios": "^0.25.0",
    "cors": "^2.8.5",
    "dotenv": "^15.0.0",
    "express": "^4.17.2",
    "mongoose": "^6.1.9",
    "morgan": "^1.10.0",
    "winston": "^3.5.1"
  },
  "devDependencies": {
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.0",
    "jest": "^27.4.7",
    "lint-staged": "^12.3.2",
    "prettier": "^2.5.1",
    "supertest": "^6.2.2"
  }
}
